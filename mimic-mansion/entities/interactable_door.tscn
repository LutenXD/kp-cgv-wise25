[sub_resource type="GDScript" id="GDScript_wfpq4"]
script/source = "extends Node3D


var open: bool = false
var player: Player


@onready var animation_player: AnimationPlayer = $AnimationPlayer
@onready var interactable_component: InteractableComponent = $AnimatableBody3D/InteractableComponent


func _ready() -> void:
	player = get_tree().get_first_node_in_group(\"Player\")


func _process(_delta: float) -> void:
	$AnimatableBody3D/Label3D.visible = is_same(interactable_component, player.current_interaction)
	$AnimatableBody3D/Label3D2.visible = is_same(interactable_component, player.current_interaction)


func _on_interactable_component_interacted() -> void:
	if open:
		animation_player.play_backwards(\"open\")
	else:
		animation_player.play(\"open\")
	
	open = !open
"

